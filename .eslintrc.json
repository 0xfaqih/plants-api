{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "indent": "off",
        "no-console":"off",
        "no-unused-vars": "off",
        "no-use-before-define":"off"
    }
}
